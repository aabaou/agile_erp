        },
        {
          "name": "St√©phane Brichler (Brichler67)",
          "email": "stephane.brichler@actency.fr",
          "role": "Developer"
        },
        {
          "name": "Ruben Petrosjan",
          "email": "ruben.petrosjan@actency.fr",
          "role": "Developer"
