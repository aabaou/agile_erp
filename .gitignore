# Ignore directories generated by Composer
/web/core
/web/modules/contrib
/web/themes/contrib
/web/profiles/contrib

/web/autoload.php
/web/index.php
/web/update.php

# Ignore Drupal's file directory
/web/sites/*/files
/data
!/data/files/private/.gitkeep
!/data/files/private/.htaccess
!/data/files/public/.gitkeep
!/data/files/public/.htaccess

# Ignore local settings file
/config/drupal/settings.local.php
/config/drupal/local/*.yml
/config/drupal/services.local.yml

# Ignore Drupal contrib modules, profiles, themes and libraries
/web/libraries/*
/web/modules/contrib/*
/web/profiles/contrib/*
/web/themes/contrib/*

# Ignore vendors
vendor

# Ignore bin
#/bin/*

# Ignore local doc generated into public repository
/web/docs

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Application specific files
node_modules/
.sass-cache/
.idea/
composer.lock
.composer

# Local docker compose file
# docker-compose.yml

# System files
._*
.DS_Store
Desktop.ini
Thumbs.db